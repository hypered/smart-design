.c-navbar.c-navbar--bordered-bottom
    .c-toolbar.c-toolbar--medium
        .c-toolbar__left
            .c-toolbar__item
                a(href="/")
                    img(src='/images/logo.svg' alt='Smart Coop Logo')
            .c-toolbar__item
                nav
                    ul.c-bordered-list-horizontal
                        -
                            var navItems = [
                                {
                                    pathName: '#',
                                    name: 'Dashboard',
                                },
                                {
                                    pathName: '#',
                                    name: 'Quotes & invoices',
                                },
                                {
                                    pathName: '#',
                                    name: 'Funding',
