                                {
                                    pathName: '#',
                                    name: 'Contracts',
                                },
                                {
                                    pathName: '#',
                                    name: 'Contracts Covid-19',
                                },
                                {
                                    pathName: '#',
                                    name: 'Equipment',
                                },
                                {
                                    pathName: '#',
                                    name: 'Activities',
                                },
                                {
                                    pathName: '#',
                                    name: 'Members',
                                },
                                {
                                    pathName: '#',
                                    name: 'Archive',
                                },
                                {
                                    pathName: '#',
                                    name: 'Detachement',
                                },
                            ]

                        each item in navItems
                            li(class=item.name==moduleTitle ? 'c-sidebar__item--active' : '')
                                a(href=item.pathName)
                                    .c-sidebar-item__label #{item.name}

        .c-toolbar__right
            .c-toolbar__item
                .c-avatar-and-text
                    +c-avatar({size: 'small'})
                    .c-avatar-and-text__text
                        p Jane Cooper

